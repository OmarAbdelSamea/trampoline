IMPLEMENTATION stm32f429 {
  OS {
    BOOLEAN [
      TRUE {
        ENUM [
          gpt,
          spi,
          V2X_Fac,
          blink_rte, 
          rte_util, 
          SWC1_Led_Test,
          V2X,
          ecdsa,
          v2xFull
        ] LIBRARY[];
      },
      FALSE
    ] BUILD = FALSE;
  };

  DEVICE_KIND [] {
    STRUCT {
      UINT32 OFFSET;
      ENUM [ BYTE, HALFWORD, WORD, DOUBLEWORD ] SIZE;
      STRUCT {
        UINT32 LOC;
      } BIT [];
      STRUCT {
        UINT32 START;
        UINT32 STOP;
      } FIELD [];
    } REGISTER [];
    STRUCT {
      IDENTIFIER ENABLE [];
      BOOLEAN ACK = FALSE;
    } EVENT [];
  };
  
  DEVICE [] {
    DEVICE_KIND_TYPE KIND;
    UINT32 ADDRESS;
    INTERRUPT_TYPE VECTOR;
  };
};

